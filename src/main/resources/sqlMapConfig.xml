<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
    <settings>
        <setting name="cacheEnabled" value="true" />
        <setting name="useGeneratedKeys" value="true" />
        <setting name="defaultExecutorType" value="REUSE" />
        <setting name="lazyLoadingEnabled" value="false" />
        <setting name="defaultStatementTimeout" value="25000" />
    </settings>

    <typeAliases>
        <typeAlias alias="User" type="com.springboot.shiro.dao.bean.User"/>
        <typeAlias alias="RouteResource" type="com.springboot.shiro.dao.bean.RouteResource"/>
        <typeAlias alias="DailyReport" type="com.springboot.shiro.dao.bean.DailyReport"/>
        <typeAlias alias="StudentEpidemicInformation" type="com.springboot.shiro.dao.bean.StudentEpidemicInformation"/>
        <typeAlias alias="Classes" type="com.springboot.shiro.dao.bean.Classes"/>
        <typeAlias alias="SystemMenu" type="com.springboot.shiro.dao.bean.SystemMenu"/>
        <typeAlias alias="Trip" type="com.springboot.shiro.dao.bean.Trip"/>
        <typeAlias alias="SchoolEpidemic" type="com.springboot.shiro.dao.bean.SchoolEpidemic"/>
        <typeAlias alias="JwcAccount" type="com.springboot.shiro.dao.bean.JwcAccount"/>
        <typeAlias alias="area" type="com.springboot.shiro.dao.bean.Area"/>
    </typeAliases>

    <mappers>
        <mapper resource="com/mysql/mapping/UserMapper.xml" />
        <mapper resource="com/mysql/mapping/RouteResourceMapper.xml" />
        <mapper resource="com/mysql/mapping/DailyReport.xml" />
        <mapper resource="com/mysql/mapping/StudentEpidemicInformation.xml" />
        <mapper resource="com/mysql/mapping/Classes.xml" />
        <mapper resource="com/mysql/mapping/SystemMenu.xml" />
        <mapper resource="com/mysql/mapping/Trip.xml" />
        <mapper resource="com/mysql/mapping/SchoolEpidemic.xml" />
        <mapper resource="com/mysql/mapping/JwcAccount.xml" />
        <mapper resource="com/mysql/mapping/area.xml" />
    </mappers>

</configuration>